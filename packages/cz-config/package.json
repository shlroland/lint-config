{
  "name": "@shlroland/cz-config",
  "type": "module",
  "version": "2.1.1",
  "description": "cz config for @shlroland/lint-config",
  "author": "shlroland <shlroland1995@gmail.com> (https://github.com/shlroland/)",
  "license": "MIT",
  "repository": "https://github.com/shlroland/lint-config",
  "keywords": [],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./gitmoji": {
      "import": "./lib/gitmoji.js",
      "require": "./lib/gitmoji.cjs"
    },
    "./conventional": {
      "import": "./lib/conventional.js",
      "require": "./lib/conventional.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/config-conventional": ">= 19",
    "commitlint-config-czg-gitmoji": "*",
    "cz-git": "^1.7.0",
    "cz-git-gitmoji-prompts": "*",
    "tsup": "^8.3.0"
  }
}
